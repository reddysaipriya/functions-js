    <script>
       function greet(){
            console.log("Hello Rishi")
        }
        greet()
        console.log("Hi Chintu")
        greet() 

       function g(name){
            console.log("Hello "+ name)
        }
        let name=prompt("Enter your Name")
        g(name) 

        function add(a,b){
            console.log(a+b)
        }
        add(3,5)
        add(9,7) 

        function a(aa,bb){
            return aa+bb
        }
        let n1=parseFloat(prompt("Enter a number1"))
        let n2=parseFloat(prompt("Enter a number2"))
        let result = a(n1,n2)
        console.log("The sum is: "+result) 

        sum= function(n1,n2){
            let r=n1+n2
            return r
        }
        console.log(sum(8,9)) 

    </script>
</body>
</html>
